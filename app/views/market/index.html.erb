<% provide :title, 'Marketplace' %>
<h1>Marketplace</h1>
<% @offers.each do |offer| %>
  <div class="well">
        <h3><%=offer.club  %>     </h3>
        <%= offer.description %>   <br>
        Position: <%=  offer.position %>
        <div id="btn-contact">
            <%= link_to 'Kontakt',new_offer_contact_path(offer_id: offer) ,class:'btn btn-primary' %>
        </div>
    </div>

<% end  %>